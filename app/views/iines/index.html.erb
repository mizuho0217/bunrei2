<p id="notice"><%= notice %></p>

<h1>Listing Iines</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Example</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iines.each do |iine| %>
      <tr>
        <td><%= iine.user_id %></td>
        <td><%= iine.example_id %></td>
        <td><%= link_to 'Show', iine %></td>
        <td><%= link_to 'Edit', edit_iine_path(iine) %></td>
        <td><%= link_to 'Destroy', iine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iine', new_iine_path %>
